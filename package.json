{
  "name": "templates",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "cross-env NODE_ENV=production rsbuild build",
    "preview": "rsbuild preview",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "jest --coverage --runInBand",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx src --fix",
    "lint": "pnpm run eslint && pnpm run stylelint",
    "lint:fix": "pnpm run eslint:fix && pnpm run stylelint:fix",
    "stylelint": "stylelint **/*.{css,scss}",
    "stylelint:fix": "stylelint --fix **/*.{css,scss}",
    "docker:build": "docker build -t templates .",
    "docker:start": "docker run -p 3000:9000 --env-file ./.env templates",
    "build-and-preview": "npm run build && npm run preview",
    "prestart": "react-env --",
    "start": "rsbuild dev",
    "posttest": "cross-env NODE_ENV=development",
    "test:coverage:ci": "jest --coverage --ci --silent --runInBand",
    "test:watch": "jest --watchAll --runInBand",
    "test:specific": "jest --watch -i ",
    "test:specific-coverage": "jest --watch -i --coverage",
    "test:clear-cache": "jest --clearCache"
  },
  "dependencies": {
    "@architecture-it/azure-b2c": "2.0.2",
    "@architecture-it/core": "^1.11.0",
    "@architecture-it/date-picker": "^2.0.0",
    "@architecture-it/react-auth": "^1.7.0",
    "@architecture-it/stylesystem": "^3.2.2",
    "@elastic/apm-rum": "^5.16.1",
    "@elastic/apm-rum-react": "^2.0.3",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/icons-material": "^5.16.3",
    "@mui/x-date-pickers": "^7.10.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/redux-mock-store": "^1.0.6",
    "AdapterDayjs": "link:@mui/x-date-pickers/AdapterDayjs",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.11",
    "formik": "^2.4.6",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.25.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@architecture-it/eslint-config-andreani": "^1.6.2",
    "@architecture-it/react-env": "1.2.2",
    "@architecture-it/rsbuild": "^1.5.1",
    "@babel/core": "^7.24.9",
    "@fortawesome/pro-solid-svg-icons": "^6.6.0",
    "@mui/material": "^5.16.3",
    "@rsbuild/core": "^0.7.10",
    "@rsbuild/plugin-react": "^0.7.10",
    "@stylistic/stylelint-config": "1.0.1",
    "@swc/core": "^1.6.13",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.39",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "axios": "^1.7.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^22.1.0",
    "msw": "^1.3.3",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.77.8",
    "stylelint": "16.1.0",
    "stylelint-config-standard": "36.0.0",
    "stylelint-config-standard-scss": "13.0.0",
    "typescript": "5.1.6"
  },
  "eslintConfig": {
    "extends": [
      "@architecture-it/andreani/react-ts"
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard-scss",
      "@stylistic/stylelint-config"
    ],
    "rules": {
      "selector-class-pattern": null
    },
    "ignoreFiles": [
      "**/*.tsx",
      "**/*.jsx",
      "**/*.js",
      "**/*.ts",
      "build/**",
      "coverage/**/*.css",
      "dist/**",
      "node_modules/**"
    ]
  }
}